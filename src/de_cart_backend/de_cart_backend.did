type Product = record {
  sku : text;
  merchant_id : text;
  image_url : text;
  name : text;
  description : text;
  price : nat;
};
service : {
  add_product : (Product) -> ();
  get_product : (text) -> (opt Product) query;
  list_products : () -> (vec record { text; Product }) query;
  remove_product : (text) -> (opt Product);
}